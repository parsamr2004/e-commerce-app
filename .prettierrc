{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "tailwindFunctions": ["clsx", "classnames", "cva"],
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"]
}
